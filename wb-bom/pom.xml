        </dependency>
        <dependency>
            <groupId>wang.52jing</groupId>
            <artifactId>wb-model-entity</artifactId>
        </dependency>
        <dependency>
            <groupId>wang.52jing</groupId>
            <artifactId>wb-model-flex</artifactId>
        </dependency>
        <dependency>
            <groupId>wang.52jing</groupId>
            <artifactId>wb-spring-boot-starter</artifactId>
        </dependency>
    </dependencies>

</project>